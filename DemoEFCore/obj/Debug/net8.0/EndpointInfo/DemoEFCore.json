          1,
          2,
          3,
          4,
          5,
          6
        ],
        "type": "integer",
        "format": "int32"
      },
      "Producto": {
        "required": [
          "nombre"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "nombre": {
            "maxLength": 100,
            "minLength": 1,
            "type": "string"
          },
          "precio": {
            "maximum": 99999.99,
            "minimum": 0,
            "type": "number",
            "format": "double"
          },
          "stock": {
            "maximum": 2147483647,
            "minimum": 0,
            "type": "integer",
            "format": "int32"
          },
          "categoriaId": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "ProductoDTO": {
        "required": [
          "categoriaId",
          "nombre"
        ],
        "type": "object",
        "properties": {
          "nombre": {
            "maxLength": 100,
            "minLength": 0,
            "type": "string"
          },
          "precio": {
            "maximum": 10000,
            "minimum": 1,
            "type": "number",
            "format": "double"
          },
          "stock": {
            "maximum": 1000,
            "minimum": 0,
            "type": "integer",
            "format": "int32"
          },
          "categoriaId": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
